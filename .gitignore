// Programa Vai ter Copa //
 
#include<stdio.h>
#include <stdlib.h>
 
int main(){
int a;
 
 // Condição: Enquanto o valor digitado pelo úsuario for diferente de EOF, ele vair se repetir //
  
while((scanf("%d",&a))!=EOF){
        if(a>0){
                printf("vai ter duas!\n");
        }
         
        else {
                printf("vai ter copa!\n");
        }
}
 
// Para encerrar o  programa digitar Ctrl-Z // 
 
system ("pause");
return 0;
}
